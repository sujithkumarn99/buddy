create table jobs(
    jobcode VARCHAR2(10) primary key,
    skill VARCHAR2(40) NOT NULL,
    experience VARCHAR2(20) NOT NULL,
    location VARCHAR2(40) NOT NULL,
    sname VARCHAR2(40) NOT NULL,
    smailid VARCHAR2(40) NOT NULL);
    
    select * from user_sequences;
    
    
CREATE SEQUENCE jobidgeneration
START WITH 20000
INCREMENT BY 2;




